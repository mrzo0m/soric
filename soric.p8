version 18
__lua__
-- soric man
#include main.lua
soric_sp=5
soric_flip=false
soric_anim_time=0
function _init()


end

function _update()

 local cur_time = time()
 
 if cur_time - soric_anim_time > 0.2 then
			soric_anim_time = time()

		 if btn(⬇️) then 
		  soric_flip=true 
		 end
		 
		 if btn(❎) then
		  soric_sp= soric_sp + 2
		 end
		 if soric_sp > 9 then
		  soric_sp=5
		 end
		 	
	
 end
 
 
end

function _draw()
  cls()
  spr(soric_sp,56,56,2,2,true)

	local i = myclass.new(5)
	-- tbl:name(arg) is a shortcut for tbl.name(tbl, arg), except tbl is evaluated only once
	print(i:get_value()) --> 5
	i:set_value(6)
	print(i:get_value()) --> 6
end


